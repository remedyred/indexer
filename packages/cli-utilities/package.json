{
	"name": "@remedyred/cli-utilities",
	"version": "0.2.8",
	"description": "CLI helpers for @remedyred/cli",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"dev": "pnpm run build --sourcemap",
		"index": "indexer",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run build --minify",
		"watch": "pnpm run dev --watch"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@hapi/topo": "^5.1.0",
		"@snickbit/cycle": "^0.0.21",
		"@snickbit/node-utilities": "^4.3.7",
		"@snickbit/out": "^2.0.27",
		"@snickbit/queue": "^1.1.5",
		"@snickbit/utilities": "^3.0.21",
		"ansi-styles-template": "^1.0.0",
		"conventional-recommended-bump": "^6.1.0",
		"execa": "^6.1.0",
		"lilconfig": "^2.0.5",
		"log-update": "^5.0.1",
		"semver": "^7.3.7",
		"workspace-tools": "^0.19.0"
	},
	"devDependencies": {
		"@remedyred/indexer": "workspace:^2.4.1",
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.35",
		"jest": "^27.5.1",
		"shx": "^0.3.4",
		"ts-jest": "^27.1.4",
		"typescript": "^4.6.3"
	},
	"engines": {
		"node": ">= 12"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
