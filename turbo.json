{
	"$schema": "https://turborepo.org/schema.json",
	"baseBranch": "origin/main",
	"pipeline": {
		"clean": {
			"outputs": []
		},
		"depcheck": {
			"outputs": [],
			"inputs": [
				"package.json"
			]
		},
		"lint": {
			"outputs": []
		},
		"fix": {
			"outputs": []
		},
		"build": {
			"dependsOn": [
				"^build"
			],
			"inputs": [
				"src/**/*.ts",
				"src/**/*.js"
			],
			"outputs": [
				"dist/**",
				"bin/**"
			]
		},
		"prod": {
			"dependsOn": [
				"^build"
			],
			"inputs": [
				"src/**/*.ts",
				"src/**/*.js"
			],
			"outputs": [
				"dist/**",
				"bin/**"
			]
		},
		"test": {
			"dependsOn": [
				"^build"
			],
			"inputs": [
				"src/**/*.ts",
				"src/**/*.js",
				"__tests__/**/*.ts",
				"__tests__/**/*.js"
			],
			"outputs": []
		},
		"prerelease": {
			"dependsOn": [
				"^prod"
			]
		},
		"docs": {
			"outputs": [
				"docs/**"
			]
		}
	}
}
