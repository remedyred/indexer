{
	"$schema": "https://turborepo.org/schema.json",
	"baseBranch": "origin/main",
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				".next/**"
			]
		},
		"prod": {
			"cache": false,
			"dependsOn": [
				"^build"
			]
		},
		"test": {
			"dependsOn": [
				"^build"
			],
			"outputs": []
		},
		"clean": {},
		"release": {},
		"docs": {},
		"dev": {
			"cache": false
		}
	}
}
